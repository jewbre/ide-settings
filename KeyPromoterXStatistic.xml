<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Debug 'Docker'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌃D'</ShortCut>
          <Description>Debug 'Docker'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>20</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open Command Dialog">
        <Item>
          <IdeaActionID>com.github.jk1.ytplugin.commands.OpenCommandWindowAction</IdeaActionID>
          <ShortCut>'⇧⌘Y'</ShortCut>
          <Description>Open Command Dialog</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open Task...">
        <Item>
          <IdeaActionID>tasks.goto</IdeaActionID>
          <ShortCut>'⌥⇧N'</ShortCut>
          <Description>Open Task...</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reload Page">
        <Item>
          <IdeaActionID>Console.TableResult.Reload</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Reload Page</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Docker'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'Docker'</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Time Tracking">
        <Item>
          <IdeaActionID>ActivateTimeTrackingToolWindow</IdeaActionID>
          <ShortCut>'⌘8'</ShortCut>
          <Description>Time Tracking</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>20</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>